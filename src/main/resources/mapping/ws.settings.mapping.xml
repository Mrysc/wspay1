<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rltx.wspay.account.dao.SettingsDao">
    <select id="select" parameterType="String" resultType="com.rltx.wspay.account.entity.SettingsEntity">
        SELECT
            param_key as paramKey,
            param_val as paramVal,
            param_comment as paramComment
        FROM
            ws_cloud_pay.ws_settings
    </select>
</mapper>